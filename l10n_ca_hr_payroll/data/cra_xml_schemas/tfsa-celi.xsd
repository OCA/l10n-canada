			<xsd:element name="FLR_ADDR" type="CanadaAddressType" minOccurs="0"/>
			<xsd:element name="CNTC" type="TFSA_ContactType"/>
			<xsd:element name="TFSA_TAMT" type="TFSATotalsType" minOccurs="0"/>
		</xsd:all>
